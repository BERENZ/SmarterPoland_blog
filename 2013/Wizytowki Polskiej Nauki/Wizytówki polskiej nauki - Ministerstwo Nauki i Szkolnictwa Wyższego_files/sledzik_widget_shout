var show_window=function(){var b=nk_wg.host+"sledzik/widget?content="+encodeURIComponent(nk_wg.content||document.location.href);var a=300;var d=600;var f=Math.floor(window.innerWidth/2-d/2);var e=Math.floor(window.innerHeight/2-a/2);var c="height="+a+",width="+d+",left="+f+",top="+e+"resizable=no,status=no,location=no";window.open(b,null,c)};document.write('<a onclick="show_window()" title="Dodaj na NK" style="cursor:pointer">\n                  <img src="'+nk_wg.image+'" style="border: 0px">\n                </a>');